{% if menu %}
	<ul class="navbar-menu">
		{% for item in items %}
			<li class="{{ item.classes | join(' ') }}">
				{% if item.children | length > 0 %}
						<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
						{% include "partial/_submenu.twig" with {'items': item.children} %}
				{% else %}
				<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
				{% endif %}
			</li>
		{% endfor %}
	</ul>
{% endif %}
